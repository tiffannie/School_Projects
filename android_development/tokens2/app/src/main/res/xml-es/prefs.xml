<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <CheckBoxPreference
        android:key="music"
        android:title="Musica"
        android:summaryOn="Musica Encendida"
        android:summaryOff="Musica Apagada"
        android:defaultValue="true" />
    <ListPreference android:key="speed"
        android:title="Velocidad del Deslizo"
        android:summary="Seleciona velocidad de fichas"
        android:entries="@array/speed_labels"
        android:entryValues="@array/speed_numbers"
        android:defaultValue="10" />
</PreferenceScreen>
